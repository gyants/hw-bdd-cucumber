{
	"name": "Custom DevContainer",
	"image": "ruby-2.6.6-rails",
	"workspaceFolder": "/app",
	"extensions": [
		"rebornix.ruby",
		"ms-vscode.vscode-ruby"
	],
	"containerEnv": {
		"RAILS_ENV": "development"
	},
	"postCreateCommand": "bundle install",
	"postStartCommand": "rails server -b 0.0.0.0"
}